<!--<?xml version="1.0"?>-->
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

 <xacro:macro name="middleware" params="link robot">

 </xacro:macro>

 <xacro:if value="${sensor in ['ft_sensor']}" >
    <xacro:include filename="$(find centauro_gazebo)/sensors/${middleware}/${sensor}.xacro"/>
 </xacro:if>

</robot>

