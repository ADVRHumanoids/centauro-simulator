</xacro:if-->

	<gazebo reference="j_arm1_7">
		<sensor type="force_torque" name="left_arm_ft">
			<force_torque>
                       <frame>sensor</frame>
                       <measure_direction>child_to_parent</measure_direction>
                     </force_torque>
			<always_on>true</always_on>
			<visualize>true</visualize>
			<update_rate>1000</update_rate>
                     <pose>0 0 -0.0565 0 0 0</pose>
		</sensor>
	</gazebo>

	<gazebo reference="j_arm2_7">
		<sensor type="force_torque" name="right_arm_ft">
			<force_torque>
                       <frame>sensor</frame>
                       <measure_direction>child_to_parent</measure_direction>
                     </force_torque>
			<always_on>true</always_on>
			<visualize>true</visualize>
			<update_rate>1000</update_rate>
                     <pose>0 0 -0.0565 0 0 0</pose>
		</sensor>
	</gazebo>

  <link name="left_arm_ft"/>
  <link name="right_arm_ft"/>
  <joint name="l_arm_ft_joint" type="fixed">
    <parent link="arm1_7"/>
    <child link="left_arm_ft"/>
    <origin rpy="0 0 0" xyz="0 0 -0.0565"/>
  </joint>
  <joint name="r_arm_ft_joint" type="fixed">
    <parent link="arm2_7"/>
    <child link="right_arm_ft"/>
    <origin rpy="0 0 0" xyz="0 0 -0.0565"/>
  </joint>

