<robot name="SE2_model">
    
    <link name="world"/>
    <link name="base_link">
        <visual>
            <geometry>
                <box size="0.2 0.2 0.1"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <box size="0.2 0.2 0.1"/>
            </geometry>
        </collision>
    </link>
    <joint name="j_x" type="prismatic">
        <parent link="world"/>
        <child link="base_link_intermediate1"/>
        <origin rpy="0 0 0" xyz="0 0 0.1"/>
        <limit effort="1000" lower="-0.5" upper="5.5" velocity="1.0"/> 
        <axis xyz="1 0 0"/>
    </joint>
    <link name="base_link_intermediate1"/>
    <joint name="j_y" type="prismatic">
        <parent link="base_link_intermediate1"/>
        <child link="base_link_intermediate2"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <limit effort="1000" lower="-3" upper="3.0" velocity="1.0"/>
        <axis xyz="0 1 0"/>
    </joint>
    <link name="base_link_intermediate2"/>
    <joint name="j_rot" type="revolute">
        <parent link="base_link_intermediate2"/>
        <child link="base_link"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <limit effort="1000" lower="-3.14" upper="3.14" velocity="1.0"/>
        <axis xyz="0 0 1"/>
    </joint>
    
    
</robot>